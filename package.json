    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "\\.snap$"
    ],
    "setupFiles": [
      "<rootDir>/__mocks__/mock-setup.js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|ico|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/imageMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/imageMock.js"
    }
  }
